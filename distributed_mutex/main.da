import subprocess
import sys
import random

def main():
    num_procs = int(sys.argv[1]) if len(sys.argv) > 1 else 10
    num_requests = int(sys.argv[2]) if len(sys.argv) > 2 else 1
    num_runs = int(sys.argv[3]) if len(sys.argv) > 3 else 1

    for _ in range(num_runs):
        subprocess.call("python3 -m da lamport.da {} {}".format(
                        random.randint(2, num_procs),
                        random.randint(1, num_requests)).split(" "))
